    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.6",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.4",
